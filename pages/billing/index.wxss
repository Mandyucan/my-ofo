/* pages/billing/index.wxss */
.container {
  display: flex;
  width: 100%;
  height:100vh;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.number, .endride {
  padding: 60rpx 0;
  flex:2;
  width: 100%;
  text-align: center;
}
.time {
  flex:6;
  width: 100%;
  text-align: center;
}
.time .time-content {
  font-size: 100rpx;
}
.endride button {
  margin-top: 40rpx;
  width: 90%;
}